{
  "manifest_version": 3,
  "name": "YouTube Embedder",
  "version": "1.0",
  "permissions": ["tabs", "activeTab", "scripting"],
  "host_permissions": ["https://www.youtube.com/*"],
  "action": {
    "default_icon": {
      "48": "images/icons8-youtube-48.png",
      "96": "images/icons8-youtube-96.png",
      "144": "images/icons8-youtube-144.png",
      "192": "images/icons8-youtube-192.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}
