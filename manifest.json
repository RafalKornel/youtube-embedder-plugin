{
  "manifest_version": 3,
  "name": "YouTube ad bypass",
  "version": "1.0",
  "description": "Jump into youtube video without ads",
  "icons": {
    "16": "images/icons8-youtube-48.svg",
    "32": "images/icons8-youtube-48.svg",
    "48": "images/icons8-youtube-48.svg",
    "128": "images/icons8-youtube-48.svg"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["scripts/sourcePageScript.js"],
      "run_at": "document_end"
    },
    {
      "matches": ["<all_urls>"],
      "js": ["scripts/targetPageScript.js"],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "background": {
    "service_worker": "scripts/background.js"
  },
  "action": {
    "default_icon": {
      "16": "images/icons8-youtube-48.svg",
      "32": "images/icons8-youtube-48.svg",
      "48": "images/icons8-youtube-48.svg",
      "128": "images/icons8-youtube-48.svg"
    }
  },
  "permissions": ["activeTab", "scripting"]
}
